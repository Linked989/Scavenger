b' <!DOCTYPE html> <html lang="en"> <head> <meta charset="utf-8"> <meta http-equiv="X-UA-Compatible" content="IE=edge"> <meta name="viewport" content="width=device-width, initial-scale=1">  <meta name="description" content="www.paste.org - allows users to paste snippets of text, usually samples of source code, for public viewing."> <meta name="author" content="An Australian"> <link rel="icon" href="/favicon.ico"> <title>Paste code - paste.org</title>  <link href="/assets/themes/bleh/css/github-highlight.css" rel="stylesheet"> <script src="/assets/themes/bleh/js/highlight.pack.js"></script> <script>hljs.initHighlightingOnLoad();</script> </head> <body> <p><pre><code class="php">&lt;?php\n include &quot;../scripts/sql_conf.php&quot;;\n//\xd0\xbf\xd0\xb5\xd1\x80\xd0\xb5\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xbd\xd1\x8b\xd0\xb5 \xd0\xb4\xd0\xb0\xd1\x82 \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd1\x8b\n$start_group1 = new DateTime(\'2011-09-13 09:00:00\');\n\n\n//var_dump($now_date);\n//var_dump($exept_date);\n//\xd0\xb2\xd0\xb2\xd0\xb5\xd1\x81\xd1\x82\xd0\xb8 \xd0\xb4\xd0\xb0\xd1\x82\xd1\x83 \xd0\xb4\xd0\xbb\xd1\x8f \xd0\xb2\xd1\x8b\xd0\xb1\xd0\xbe\xd1\x80\xd0\xba\xd0\xb8 \xd0\xb2 sql \xd0\xb7\xd0\xb0\xd0\xbf\xd1\x80\xd0\xbe\xd1\x81\xd0\xb5\n\n//\xd0\xbf\xd1\x80\xd0\xbe\xd0\xb2\xd0\xb5\xd1\x80\xd0\xba\xd0\xb0 \xd0\xbf\xd1\x80\xd0\xb8\xd0\xbd\xd0\xb0\xd0\xb4\xd0\xbb\xd0\xb5\xd0\xb6\xd0\xbd\xd0\xbe\xd1\x81\xd1\x82\xd0\xb8 \xd0\xba \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xb5, \xd0\xbe\xd0\xbf\xd1\x80\xd0\xb5\xd0\xb4\xd0\xb5\xd0\xbb\xd0\xb5\xd0\xbd\xd0\xb8\xd0\xb5 \xd0\xb2\xd1\x8b\xd0\xb1\xd0\xbe\xd1\x80\xd0\xba\xd0\xb8, \xd0\xbf\xd0\xbe\xd0\xba\xd0\xb0\xd0\xb7 \xd1\x82\xd0\xb5\xd0\xba\xd1\x83\xd1\x89\xd0\xb5\xd0\xb9 \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd1\x8b.\nif(9&gt;date(\'H\')){//\xd0\xb2\xd1\x80\xd0\xb5\xd0\xbc\xd1\x8f \xd1\x81 \xd0\xbf\xd0\xbe\xd0\xbb\xd1\x83\xd0\xbd\xd0\xbe\xd1\x87\xd0\xb8 \xd0\xb4\xd0\xbe 9 \xd1\x83\xd1\x82\xd1\x80\xd0\xb0\n $start_date = date(\'d-m-Y\',strtotime (&quot;-1 day&quot;));\n $end_date = date(\'d-m-Y\',strtotime (&quot;now&quot;));\n $group_date = date (\'y-m-d H:i:s\', mktime(9, 0, 0, date(&quot;m&quot;), date(&quot;d&quot;)-1, date(&quot;Y&quot;)));\n $sql_date_u = mktime(9, 0, 0, date(&quot;m&quot;)  , date(&quot;d&quot;)-1, date(&quot;Y&quot;));\n $sql_date = date (\'y-m-d H:i:s\', $sql_date_u);\n $sql_date_h = date (\'d.m.y H:i\', $sql_date_u);\n $except_date = date(\'Y-m-d\',strtotime (&quot;-1 day&quot;));//\xd0\xb4\xd0\xb0\xd1\x82\xd0\xb0 \xd0\xb4\xd0\xbb\xd1\x8f \xd0\xbf\xd0\xbe\xd0\xb8\xd1\x81\xd0\xba\xd0\xb0 \xd0\xb8\xd1\x81\xd0\xba\xd0\xbb\xd1\x8e\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd0\xb9\n }\n\n else {//\xd0\xb2\xd1\x80\xd0\xb5\xd0\xbc\xd1\x8f \xd0\xbf\xd0\xbe\xd1\x81\xd0\xbb\xd0\xb5 9 \xd1\x83\xd1\x82\xd1\x80\xd0\xb0 \xd0\xb8 \xd0\xb4\xd0\xbe \xd0\xbf\xd0\xbe\xd0\xbb\xd1\x83\xd0\xbd\xd0\xbe\xd1\x87\xd0\xb8\n $start_date = date(\'d-m-Y\',strtotime (&quot;now&quot;));\n $end_date = date(\'d-m-Y\',strtotime (&quot;+1 day&quot;));\n $group_date = date (\'y-m-d H:i:s\', mktime(9, 0, 0, date(&quot;m&quot;), date(&quot;d&quot;), date(&quot;Y&quot;)));\n $sql_date_u = mktime(9, 0, 0, date(&quot;m&quot;)  , date(&quot;d&quot;), date(&quot;Y&quot;));\n $sql_date = date (\'y-m-d H:i:s\', $sql_date_u) ;\n $sql_date_h = date (\'d.m.y H:i\', $sql_date_u) ;\n $except_date = date(\'Y-m-d\',strtotime (&quot;now&quot;));//\xd0\xb4\xd0\xb0\xd1\x82\xd0\xb0 \xd0\xb4\xd0\xbb\xd1\x8f \xd0\xbf\xd0\xbe\xd0\xb8\xd1\x81\xd0\xba\xd0\xb0 \xd0\xb8\xd1\x81\xd0\xba\xd0\xbb\xd1\x8e\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd0\xb9\n}\n//echo &quot;\xd0\xa1\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xb0 $start_date - $end_date&quot;.&quot;&lt;br&gt;&quot;;\n//\xd0\xb8\xd1\x89\xd0\xb5\xd0\xbc \xd0\xb8\xd1\x81\xd0\xba\xd0\xbb\xd1\x8e\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd1\x8f \xd0\xb2 \xd0\xb3\xd1\x80\xd0\xb0\xd1\x84\xd0\xb8\xd0\xba\xd0\xb5\n//$exept=mysql_query(&quot;SELECT * FROM exception WHERE date = \'$exept_date\'&quot;);\n /*\necho $now_date;\nvar_dump($now_date);\nvar_dump($exept_date);\n  */\n\n\n//\xd0\xb2\xd1\x8b\xd1\x87\xd0\xb8\xd1\x81\xd0\xbb\xd1\x8f\xd0\xb5\xd0\xbc \xd1\x80\xd0\xb0\xd0\xb7\xd0\xbd\xd0\xb8\xd1\x86\xd1\x83 \xd0\xb2 \xd0\xb4\xd0\xbd\xd1\x8f\xd1\x85\n$datetime1 = new DateTime(&quot;$group_date&quot;);\n\n$diff = $start_group1-&gt;diff($datetime1);\n$interval=$diff-&gt;format(\'%a%\');\n$group=$interval%4;\n\n\n$online = \'\xd0\x9d\xd0\xb0 \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xb5: \';\n//\xd0\xb7\xd0\xb0\xd0\xbf\xd1\x80\xd0\xbe\xd1\x81 \xd0\xba \xd0\xb1\xd0\xb4 users \xd0\xb8 \xd0\xbf\xd0\xbe\xd0\xbb\xd1\x83\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd0\xb5 \xd0\xb4\xd0\xb0\xd0\xbd\xd0\xbd\xd1\x8b\xd0\xb9 \xd0\xba\xd1\x82\xd0\xbe \xd0\xbf\xd0\xbe\xd1\x83\xd0\xbc\xd0\xbe\xd0\xbb\xd1\x87\xd0\xb0\xd0\xbd\xd0\xb8\xd1\x8e \xd0\xbd\xd0\xb0 \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xb5\n$group = &quot;SELECT * FROM users WHERE group_id = \'$group\' AND dc_id = \'1\'&quot;;\n$date_tt = mysql_query($group);\n\nwhile ($current_group=mysql_fetch_array ($date_tt)){\n //\xd0\xbf\xd0\xbe\xd0\xb8\xd1\x81\xd0\xba \xd0\xb8\xd1\x81\xd0\xba\xd0\xbb\xd1\x8e\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd1\x8f \xd0\xbf\xd0\xbe \xd0\xb4\xd0\xb0\xd1\x82\xd0\xb5 \xd0\xb8 \xd0\x98\xd0\x94   \n $sql = mysql_query(&quot;SELECT * FROM exception WHERE date = \'&quot;.$except_date.&quot;\' AND user_id = \'&quot;.$current_group[\'user_id\'] .&quot;\'&quot;);//\xd0\xb7\xd0\xb0\xd0\xbf\xd1\x80\xd0\xbe\xd1\x81 \xd0\xba \xd0\xb1\xd0\xb4 \xd0\xb8\xd1\x81\xd0\xba\xd0\xbb\xd1\x8e\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd0\xb9 \n  if (mysql_num_rows($sql) &gt;0)  {\n   $except = mysql_fetch_array ($sql);\n   $online .= &quot;{$except[\'fio_2_change\']}&quot;.&quot;\\r&quot; ; //\xd0\xb5\xd1\x81\xd0\xbb\xd0\xb8 \xd0\xb7\xd0\xb0\xd0\xbf\xd0\xb8\xd1\x81\xd1\x8c \xd1\x81\xd1\x83\xd1\x89\xd0\xb5\xd1\x81\xd1\x82\xd0\xb2\xd1\x83\xd0\xb5\xd1\x82 \xd1\x82\xd0\xbe \xd0\xbf\xd1\x80\xd0\xb8\xd1\x81\xd0\xb2\xd0\xb0\xd0\xb8\xd0\xb2\xd0\xb0\xd0\xb5\xd0\xbc \xd0\xb7\xd0\xbd\xd0\xb0\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd0\xb5 \xd0\xb8\xd1\x81\xd0\xba\xd0\xbb\xd1\x8e\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd1\x8f \n     }\n  else {\n  $online .= &quot;{$current_group[\'fio\']}&quot;.&quot;\\r&quot; ;//echo \'\xd0\xbd\xd0\xb5 \xd1\x81\xd1\x83\xd1\x89\xd0\xb5\xd1\x81\xd1\x82\xd0\xb2\xd1\x83\xd0\xb5\xd1\x82\' ;\n  }\n}\n\n\n//$online - \xd0\xb2 \xd0\xbf\xd0\xb5\xd1\x80\xd0\xb5\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xbd\xd0\xbe\xd0\xb9 \xd0\xb7\xd0\xbd\xd0\xb0\xd1\x87\xd0\xb5\xd0\xbd\xd0\xb8\xd0\xb5 \xd0\xba\xd1\x82\xd0\xbe \xd0\xbd\xd0\xb0 \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xb5 \xd0\xb2\xd0\xb8\xd0\xb4\xd0\xb0 &quot;\xd0\x9d\xd0\xb0 \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xb5: \xd0\xa4\xd0\x98\xd0\x9e \xd0\xa4\xd0\x98\xd0\x9e&quot;\n//$start_date - \xd0\xb2 \xd0\xbf\xd0\xb5\xd1\x80\xd0\xb5\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xbd\xd0\xbe\xd0\xb9 \xd0\xb4\xd0\xb0\xd1\x82\xd0\xb0 \xd0\xbd\xd0\xb0\xd1\x87\xd0\xb0\xd0\xbb\xd0\xb0 \xd1\x82\xd0\xb5\xd0\xba\xd1\x83\xd1\x89\xd0\xb5\xd0\xb9 \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd1\x8b\n//$end_date - \xd0\xb2 \xd0\xbf\xd0\xb5\xd1\x80\xd0\xb5\xd0\xbc\xd0\xb5\xd0\xbd\xd0\xbd\xd0\xbe\xd0\xb9 \xd0\xb4\xd0\xb0\xd1\x82\xd0\xb0 \xd0\xba\xd0\xbe\xd0\xbd\xd1\x86\xd0\xb0 \xd1\x82\xd0\xb5\xd0\xba\xd1\x83\xd1\x89\xd0\xb5\xd0\xb9 \xd1\x81\xd0\xbc\xd0\xb5\xd0\xbd\xd1\x8b\n?&gt;</code></pre></p> <p align="right"><span style="font-size: 60%">Add a code snippet to your website: <a href="https://www.paste.org" target="_blank">www.paste.org</a></span></p> </body> </html>'