b' <!DOCTYPE html> <html lang="en"> <head> <meta charset="utf-8"> <meta http-equiv="X-UA-Compatible" content="IE=edge"> <meta name="viewport" content="width=device-width, initial-scale=1">  <meta name="description" content="www.paste.org - allows users to paste snippets of text, usually samples of source code, for public viewing."> <meta name="author" content="An Australian"> <link rel="icon" href="/favicon.ico"> <title>Paste code - paste.org</title>  <link href="/assets/themes/bleh/css/github-highlight.css" rel="stylesheet"> <script src="/assets/themes/bleh/js/highlight.pack.js"></script> <script>hljs.initHighlightingOnLoad();</script> </head> <body> <p><pre><code class="pascal">program fff;\nUses\n crt,graph;\nconst\n d=50; {shirina dorozhki}\ntype\n ASpeed=array[1..3] of integer; {nachalnaya skorost}\n MFlags=array[1..4] of boolean; {flagi, opredelyayut po kakoi storone bezhit sportsmen}\n Engine=record\n  AddX:integer; {prirashenie na verhnei storone}\n  AddX2:integer; {prirashenie na nizhnei storone}\n  AddY:integer; {prirashenie sprava}\n  AddY2:integer; {prirashenie sleva}\n  NCircle:shortint; {kolichestvo krugov,proidennih kazhdim uchastnikom}\n  FStop:boolean; {flag, opredelyayet kogda ostanovit sportsmena}\n  F:Mflags;\n  Color:byte; {Cvet kazhdogo sportsmena}\n  Point:string; {Mesto}\n end;\n APeople=array[1..3] of Engine;\nvar\n Driver,Mode,Error,MaxX,MaxY,x1,x2,y1,y2,c,e,i,j:integer; {x1,y1,x2,y2 - koordinaty bolshogo prymougolnika}\n Flag1,Flag2:boolean; {opredelyayut, kogda ochishat ekran}\n n,temp:byte; {n - kolichestvo krugov}\n Info:APeople;\n key:char; {klavisha}\n MaxV:ASpeed; {skorost kazhdogo sportsmena}\n\nprocedure CheckResolution(var MaxX,MaxY:integer);\nBegin\n MaxX:=GetMaxX;\n MaxY:=GetMaxY;\nend;\n\nprocedure DrawRectangle(var MaxX,MaxY,x1,x2,y1,y2:integer);\nvar\n i:byte;\nBegin\n x1:=MaxX div 150;\n y1:=MaxY div 149;\n x2:=x1*149;\n y2:=y1*148;\n for i:=0 to 3 do\n  Rectangle(x1+i*d,y1+i*d,x2-i*d,y2-i*d);\nend;\n\nprocedure DrawPeople0(var x1,y1,c:integer);\nBegin\n Circle(x1+2*d+25,y1+5+c,5);\n Line(x1+2*d+25,y1+10+c,x1+2*d+25,y1+25+c); {tulovishe}\n Line(x1+2*d+25,y1+10+c,x1+2*d+35,y1+25+c); {pravaya ruka}\n Line(x1+2*d+25,y1+10+c,x1+2*d+15,y1+25+c); {levaya ruka}\n Line(x1+2*d+25,y1+25+c,x1+2*d+35,y1+50+c); {pravaya noga}\n Line(x1+2*d+25,y1+25+c,x1+2*d+15,y1+50+c); {levaya noga}\nEnd;\n\nprocedure DrawPeople1(var x1,y1,c,e:integer);\nBegin\n Circle(x1+2*d+25+c,y1+5+e,5);\n Line(x1+2*d+25+c,y1+10+e,x1+2*d+20+c,y1+30+e); {tulovishe}\n Line(x1+2*d+25+c,y1+10+e,x1+2*d+30+c,y1+20+e); {pravaya ruka}\n Line(x1+2*d+30+c,y1+20+e,x1+2*d+40+c,y1+15+e);\n Line(x1+2*d+25+c,y1+10+e,x1+2*d+15+c,y1+15+e); {levaya ruka}\n Line(x1+2*d+15+c,y1+15+e,x1+2*d+17+c,y1+23+e);\n Line(x1+2*d+20+c,y1+30+e,x1+2*d+35+c,y1+35+e); {pravaya noga}\n Line(x1+2*d+35+c,y1+35+e,x1+2*d+30+c,y1+45+e);\n Line(x1+2*d+30+c,y1+45+e,x1+2*d+35+c,y1+47+e);\n Line(x1+2*d+20+c,y1+30+e,x1+2*d+13+c,y1+40+e); {levaya noga}\n Line(x1+2*d+13+c,y1+40+e,x1+2*d+3+c,y1+33+e);\n Line(x1+2*d+3+c,y1+33+e,x1+2*d+c,y1+39+e);\nend;\n\nprocedure DrawPeople2(var x1,y1,c,e:integer);\nBegin\n Circle(x1+125+c,y1+5+e,5);\n Line(x1+2*d+25+c,y1+10+e,x1+2*d+20+c,y1+30+e); {tulovishe}\n Line(x1+2*d+25+c,y1+10+e,x1+2*d+24+c,y1+24+e); {pravaya ruka}\n Line(x1+2*d+24+c,y1+24+e,x1+2*d+33+c,y1+21+e);\n Line(x1+2*d+25+c,y1+10+e,x1+2*d+18+c,y1+20+e); {levaya ruka}\n Line(x1+2*d+18+c,y1+20+e,x1+2*d+24+c,y1+24+e);\n Line(x1+2*d+20+c,y1+30+e,x1+2*d+25+c,y1+38+e); {pravaya noga}\n Line(x1+2*d+25+c,y1+38+e,x1+2*d+20+c,y1+46+e);\n Line(x1+2*d+20+c,y1+46+e,x1+2*d+24+c,y1+50+e);\n Line(x1+2*d+20+c,y1+30+e,x1+2*d+20+c,y1+40+e); {levaya noga}\n Line(x1+2*d+20+c,y1+40+e,x1+2*d+15+c,y1+47+e);\n Line(x1+2*d+15+c,y1+47+e,x1+2*d+18+c,y1+50+e);\nend;\n\nprocedure DrawPeople3(var x2,y1,c,e:integer);\nBegin\n Circle(x2-25-c,y1+5+e+c,5);\n Line(x2-25-c,y1+10+e+c,x2-25-c,y1+25+e+c); {tulovishe}\n Line(x2-25-c,y1+10+e+c,x2-15-c,y1+25+e+c); {pravaya ruka}\n Line(x2-25-c,y1+10+e+c,x2-35-c,y1+25+e+c); {levaya ruka}\n Line(x2-25-c,y1+25+e+c,x2-20-c,y1+40+e+c); {pravaya noga}\n Line(x2-20-c,y1+40+e+c,x2-20-c,y1+30+e+c);\n Line(x2-25-c,y1+25+e+c,x2-30-c,y1+40+e+c); {levaya noga}\n Line(x2-30-c,y1+40+e+c,x2-30-c,y1+50+e+c);\nEnd;\n\nprocedure DrawPeople4(var x2,y1,c,e:integer);\nBegin\n Circle(x2-25-c,y1+5+e+c,5);\n Line(x2-25-c,y1+10+e+c,x2-25-c,y1+25+e+c); {tulovishe}\n Line(x2-25-c,y1+10+e+c,x2-15-c,y1+25+e+c); {pravaya ruka}\n Line(x2-25-c,y1+10+e+c,x2-35-c,y1+25+e+c); {levaya ruka}\n Line(x2-25-c,y1+25+e+c,x2-20-c,y1+40+e+c); {pravaya noga}\n Line(x2-20-c,y1+40+e+c,x2-20-c,y1+50+e+c);\n Line(x2-25-c,y1+25+e+c,x2-30-c,y1+40+e+c); {levaya noga}\n Line(x2-30-c,y1+40+e+c,x2-30-c,y1+30+e+c);\nEnd;\n\nprocedure DrawPeople5(var x2,y2,c,e:integer);\nBegin\n Circle(x2-25-c-e,y2-45-e,5);\n Line(x2-25-c-e,y2-40-e,x2-20-c-e,y2-20-e); {tulovishe}\n Line(x2-25-c-e,y2-40-e,x2-15-c-e,y2-35-e); {pravaya ruka}\n Line(x2-15-c-e,y2-35-e,x2-18-c-e,y2-27-e);\n Line(x2-25-c-e,y2-40-e,x2-30-c-e,y2-30-e); {levaya ruka}\n Line(x2-30-c-e,y2-30-e,x2-36-c-e,y2-32-e);\n Line(x2-20-c-e,y2-20-e,x2-12-c-e,y2-10-e); {pravaya noga}\n Line(x2-12-c-e,y2-10-e,x2-3-c-e,y2-13-e);\n Line(x2-3-c-e,y2-13-e,x2-c-e,y2-10-e);\n Line(x2-20-c-e,y2-20-e,x2-33-c-e,y2-13-e); {levaya noga}\n Line(x2-33-c-e,y2-13-e,x2-30-c-e,y2-3-e);\n Line(x2-30-c-e,y2-3-e,x2-35-c-e,y2-e);\nend;\n\nprocedure DrawPeople6(var x2,y2,c,e:integer);\nBegin\n Circle(x2-25-c-e,y2-45-e,5);\n Line(x2-25-c-e,y2-40-e,x2-20-c-e,y2-20-e); {tulovishe}\n Line(x2-25-c-e,y2-40-e,x2-18-c-e,y2-29-e); {pravaya ruka}\n Line(x2-18-c-e,y2-29-e,x2-22-c-e,y2-28-e);\n Line(x2-25-c-e,y2-40-e,x2-27-c-e,y2-28-e); {levaya ruka}\n Line(x2-27-c-e,y2-28-e,x2-32-c-e,y2-32-e);\n Line(x2-20-c-e,y2-20-e,x2-20-c-e,y2-10-e); {pravaya noga}\n Line(x2-20-c-e,y2-10-e,x2-16-c-e,y2-3-e);\n Line(x2-16-c-e,y2-3-e,x2-18-c-e,y2-e);\n Line(x2-20-c-e,y2-20-e,x2-25-c-e,y2-10-e); {levaya noga}\n Line(x2-25-c-e,y2-10-e,x2-20-c-e,y2-3-e);\n Line(x2-20-c-e,y2-3-e,x2-25-c-e,y2-e);\nend;\n\nprocedure DrawPeople7(var x1,y2,c,e:integer);\nBegin\n Circle(x1+25+c,y2-45-e-c,5);\n Line(x1+25+c,y2-40-e-c,x1+25+c,y2-25-e-c); {tulovishe}\n Line(x1+25+c,y2-40-e-c,x1+35+c,y2-25-e-c); {pravaya ruka}\n Line(x1+25+c,y2-40-e-c,x1+15+c,y2-25-e-c); {levaya ruka}\n Line(x1+25+c,y2-25-e-c,x1+30+c,y2-18-e-c); {pravaya noga}\n Line(x1+30+c,y2-18-e-c,x1+30+c,y2-9-e-c);\n Line(x1+25+c,y2-25-e-c,x1+18+c,y2-18-e-c); {levaya noga}\n Line(x1+18+c,y2-18-e-c,x1+18+c,y2-3-e-c);\nend;\n\nprocedure DrawPeople8(var x1,y2,c,e:integer);\nBegin\n Circle(x1+25+c,y2-45-e-c,5);\n Line(x1+25+c,y2-40-e-c,x1+25+c,y2-25-e-c); {tulovishe}\n Line(x1+25+c,y2-40-e-c,x1+35+c,y2-25-e-c); {pravaya ruka}\n Line(x1+25+c,y2-40-e-c,x1+15+c,y2-25-e-c); {levaya ruka}\n Line(x1+25+c,y2-25-e-c,x1+32+c,y2-18-e-c); {pravaya noga}\n Line(x1+32+c,y2-18-e-c,x1+32+c,y2-3-e-c);\n Line(x1+25+c,y2-25-e-c,x1+20+c,y2-20-e-c); {levaya noga}\n Line(x1+20+c,y2-20-e-c,x1+20+c,y2-9-e-c);\nend;\n\nBegin\n clrscr;\n Writeln(\'Vvedite kolichestvo krugov\');\n Read(n);\n for i:=1 to 3 do\n  Begin\n   Writeln(\'Vvedite skorost \',i,\' sportsmena\');\n   Read(MaxV[i]);\n  end;\n Driver:=Detect;\n InitGraph(Driver,Mode,\'\');\n Error:=GraphResult;\n If Error&lt;&gt;grOk then\n  Writeln(GraphErrorMsg(Error))\n else\n Begin\n  CheckResolution(MaxX,MaxY);\n  for i:=1 to 3 do\n  Begin\n   Info[i].AddX:=0;\n   Info[i].AddY:=0;\n   Info[i].AddX2:=0;\n   Info[i].AddY2:=0;\n   Info[i].NCircle:=-1;\n   Info[i].FStop:=false;\n   For j:=2 to 4 do\n    Info[i].F[j]:=False;\n  end;\n  Info[1].Color:=4;\n  Info[2].Color:=10;\n  Info[3].Color:=14;\n  For i:=1 to 3 do\n   Info[i].F[1]:=True;\n  DrawRectangle(MaxX,MaxY,x1,x2,y1,y2);\n  c:=0;\n  For i:=1 to 3 do\n  Begin\n   SetColor(Info[i].Color);\n   DrawPeople0(x1,y1,c);\n   c:=c+d;\n  end;\n  key:=Readkey;\n  temp:=1;\n  If key=#13 then\n  Begin\n   Randomize;\n   While true do\n   Begin\n    c:=0;\n    SetColor(15);\n    DrawRectangle(MaxX,MaxY,x1,x2,y1,y2);\n    For i:=1 to 3 do\n    Begin\n     If  (Info[i].F[1]) and (not(Info[i].FStop)) and (Info[i].AddX+x1+150&lt;x2-c) then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople1(x1,y1,Info[i].AddX,c);\n      If (Info[i].AddX+MaxV[i]&gt;=0) and (Info[i].AddX+MaxV[i]&lt;=MaxV[i]) then\n       Info[i].NCircle:=Info[i].NCircle+1;\n      If Info[i].NCircle=n then\n      Begin\n       Info[i].FStop:=true;\n       Str(temp,Info[i].Point);\n       temp:=temp+1;\n      end;\n      Info[i].AddX:=Info[i].AddX+MaxV[i];\n     Flag1:=true;\n     End;\n     c:=c+d;\n     If Info[i].AddX+x1+100&gt;=x2-c then\n     Begin\n      Info[i].F[1]:=False;\n      Info[i].F[2]:=True;\n     End;\n    End;\n    c:=0;\n    For i:=1 to 3 do\n    Begin\n     If Info[i].FStop then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople0(x1,y1,c);\n      Flag1:=true;\n     end;\n     c:=c+d;\n    end;\n    c:=0;\n    If (Info[1].NCircle=n) and (Info[2].NCircle=n) and (Info[3].NCircle=n) then\n     break;\n    For i:=1 to 3 do\n    Begin\n     If  (Info[i].F[2]) and (not(Info[i].FStop)) and (Info[i].AddY+y1+c+50&lt;y2-c) then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople3(x2,y1,c,Info[i].AddY);\n      Info[i].AddY:=Info[i].AddY+MaxV[i];\n     Flag1:=true;\n     End;\n     If (Info[i].AddY+y1+c+50&gt;=y2-c) and (Info[i].AddY&lt;&gt;0) and (Info[i].F[1]=false) then\n     Begin\n      Info[i].F[2]:=False;\n      Info[i].F[3]:=True;\n     End;\n     c:=c+d;\n    End;\n    c:=0;\n    For i:=1 to 3 do\n    Begin\n     If  (Info[i].F[3]) and (not(Info[i].FStop)) and (x1+Info[i].AddX2+c&lt;x2-c-50) then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople5(x2,y2,Info[i].AddX2,c);\n      Info[i].AddX2:=Info[i].AddX2+MaxV[i];\n     Flag1:=true;\n     End;\n     If Info[i].AddX2+x1+c&gt;=x2-c-50 then\n     Begin\n      Info[i].F[3]:=False;\n      Info[i].F[4]:=True;\n     End;\n    c:=c+d;\n    End;\n    c:=0;\n    For i:=1 to 3 do\n    Begin\n     If  (Info[i].F[4]) and (not(Info[i].FStop)) and (y1+Info[i].AddY2+c&lt;y2-c-50) then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople7(x1,y2,c,Info[i].AddY2);\n      Info[i].AddY2:=Info[i].AddY2+MaxV[i];\n     Flag1:=true;\n     End;\n     If Info[i].AddY2+y1+c&gt;=y2-c-50 then\n     Begin\n      Info[i].F[4]:=False;\n      Info[i].F[1]:=True;\n     End;\n    c:=c+d;\n    End;\n    c:=0;\n    If Flag1 then\n    Begin\n     Delay(17000);\n    ClearDevice;\n     SetColor(15);\n     DrawRectangle(MaxX,MaxY,x1,x2,y1,y2);\n    Flag1:=false;\n    end;\n    For i:=1 to 3 do\n    Begin\n     If  (Info[i].F[1]) and (not(Info[i].FStop)) and (Info[i].AddX+x1+150&lt;x2-c) then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople2(x1,y1,Info[i].AddX,c);\n      If (Info[i].AddX+MaxV[i]&gt;=0) and (Info[i].AddX+MaxV[i]&lt;=MaxV[i]) then\n       Info[i].NCircle:=Info[i].NCircle+1;\n      If Info[i].NCircle=n then\n      Begin\n       Info[i].FStop:=true;\n       Str(temp,Info[i].Point);\n       Temp:=temp+1;\n      end;\n      Info[i].AddX:=Info[i].AddX+MaxV[i];\n     Flag2:=true;\n     End;\n     If Info[i].AddX+x1+100&gt;=x2-c then\n     Begin\n      Info[i].F[1]:=False;\n      Info[i].F[2]:=True;\n     End;\n     c:=c+d;\n    End;\n    If (Info[1].NCircle=n) and (Info[2].NCircle=n) and (Info[3].NCircle=n) then\n     break;\n    c:=0;\n    For i:=1 to 3 do\n    Begin\n     If  (Info[i].F[2]) and (not(Info[i].FStop)) and (Info[i].AddY+y1+c+50&lt;y2-c) then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople4(x2,y1,c,Info[i].AddY);\n      Info[i].AddY:=Info[i].AddY+MaxV[i];\n     Flag2:=true;\n     End;\n     If (Info[i].AddY+y1+c+50&gt;=y2-c) and (Info[i].AddY&lt;&gt;0) and (Info[i].F[1]=false) then\n     Begin\n      Info[i].F[2]:=False;\n      Info[i].F[3]:=True;\n     End;\n     c:=c+d;\n    End;\n    c:=0;\n    For i:=1 to 3 do\n    Begin\n     If  (Info[i].F[3]) and (not(Info[i].FStop)) and (x1+Info[i].AddX2+c&lt;x2-c-50) then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople6(x2,y2,Info[i].AddX2,c);\n      Info[i].AddX2:=Info[i].AddX2+MaxV[i];\n     Flag2:=true;\n     End;\n     If Info[i].AddX2+x1+c&gt;=x2-c-50 then\n     Begin\n      Info[i].F[3]:=False;\n      Info[i].F[4]:=True;\n     End;\n    c:=c+d;\n    End;\n    c:=0;\n    For i:=1 to 3 do\n    Begin\n     If  (Info[i].F[4]) and (not(Info[i].FStop)) and (y1+Info[i].AddY2+c&lt;y2-c-50) then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople8(x1,y2,c,Info[i].AddY2);\n      Info[i].AddY2:=Info[i].AddY2+MaxV[i];\n     Flag2:=true;\n     End;\n     If Info[i].AddY2+y1+c&gt;=y2-c-50 then\n     Begin\n      Info[i].F[4]:=False;\n      Info[i].F[1]:=True;\n      Info[i].AddX:=-100+c;\n      Info[i].AddY:=0;\n      Info[i].AddX2:=0;\n      Info[i].AddY2:=0;\n     End;\n    c:=c+d;\n    End;\n    c:=0;\n    For i:=1 to 3 do\n    Begin\n     If Info[i].FStop then\n     Begin\n      SetColor(Info[i].Color);\n      DrawPeople0(x1,y1,c);\n      Flag1:=true;\n     end;\n     c:=c+d;\n    end;\n    If Flag2 then\n    Begin\n     Delay(17000);\n     ClearDevice;\n     SetColor(15);\n     DrawRectangle(MaxX,MaxY,x1,x2,y1,y2);\n    Flag2:=false;\n    end;\n    c:=0;\n   End;\n   c:=0;\n   ClearDevice;\n   SetColor(15);\n   DrawRectangle(MaxX,MaxY,x1,x2,y1,y2);\n   For i:=1 to 3 do\n    Begin\n     SetColor(Info[i].Color);\n     DrawPeople0(x1,y1,c);\n     OutTextXY(x1+150,y1+c+25,Info[i].Point);\n     c:=c+d;\n    end;\n  End;\n End;\n Readkey;\nEnd.</code></pre></p> <p align="right"><span style="font-size: 60%">Add a code snippet to your website: <a href="https://www.paste.org" target="_blank">www.paste.org</a></span></p> </body> </html>'