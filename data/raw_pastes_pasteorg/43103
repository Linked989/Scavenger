 elementow mniejszych od sredniej\n        max1=t[i];\n    }\n    if(t[i]&gt;=srednia)\n    {\n        if(t[i]&lt;min1)         // Znajduje min1(element najmniejszy z elementow wiekszych od sredniej\n        min1=t[i];\n    }\n}\n\nes1=srednia-max1;   // Oblicza roznice miedzy srednia a max1 i srednia a min1\nes2=min1-srednia;   //\nif(es1&lt;es2)\nprintf(&quot;Element sredni jest rowny %dn&quot;,max1); // Dla najmniejszej roznicy wypisuje element sredni\nif(es1&gt;es2)\nprintf(&quot;Element sredni jest rowny %dn&quot;,min1);\nif(es1==0 &amp;&amp; es2==0)\nprintf(&quot;Element sredni jest rowny %dn&quot;,min1);\n\n//----------------------------------------------------------\nif(N%2==0) printf(&quot;Nie ma elementu srodkowego, poniewaz wystepuje parzysta liczba komorek tablicy.&quot;);\n else\n {\n for(i=0; i&lt;N; i++)\n {\n min=max=0;\n for(j=0; j&lt;N; j++)\n {\n if(t[i]&lt;=t[j]) min++;\n if(t[i]&gt;=t[j]) max++;\n }\n if(min==max) printf(&quot;Elementem srodkowym jest %d&quot;,t[i]);\n }\n }\n return 0;\n}\n</code></pre></p> <p align="right"><span style="font-size: 60%">Add a code snippet to your website: <a href="https://www.paste.org" target="_blank">www.paste.org</a></span></p> </body> </html>'