b' <!DOCTYPE html> <html lang="en"> <head> <meta charset="utf-8"> <meta http-equiv="X-UA-Compatible" content="IE=edge"> <meta name="viewport" content="width=device-width, initial-scale=1">  <meta name="description" content="www.paste.org - allows users to paste snippets of text, usually samples of source code, for public viewing."> <meta name="author" content="An Australian"> <link rel="icon" href="/favicon.ico"> <title>Paste code - paste.org</title>  <link href="/assets/themes/bleh/css/github-highlight.css" rel="stylesheet"> <script src="/assets/themes/bleh/js/highlight.pack.js"></script> <script>hljs.initHighlightingOnLoad();</script> </head> <body> <p><pre><code class="javascript">// Concerte examples\r\n// [1, 2],[4, 1] =&gt; true (order of items does not matter)\r\n// [1],[3, 3] =&gt; false =&gt; length does matter\r\n// [1, 2 ,2] =&gt; [4, 1, 4] =&gt; Frequency does matter\r\n\r\n// O(n^2)  worst preformance\r\nfunction squareFrequency(arr1, arr2) {\r\n    if (arr1.length !== arr2.length) {\r\n        return false;\r\n    }\r\n\r\n    for (var value of arr1) {\r\n        const correspondSquareIndex = arr2.indexOf(value ** 2);\r\n\r\n        if (correspondSquareIndex === -1) {\r\n            return false;\r\n        }\r\n\r\n        arr2.splice(correspondSquareIndex, 1);\r\n    }\r\n\r\n    return true;\r\n}\r\n\r\n// O(n) best performance\r\nfunction squareFrequency (arr1, arr2) {\r\n    if (arr1.length !== arr2.length) {\r\n        return false;\r\n    }\r\n\r\n    const hash1 = {};\r\n    const hash2 = {};\r\n\r\n    for (const value of arr1) {\r\n        hash1[value] = (hash1[value] || 0) + 1\r\n    }\r\n\r\n    for (const value of arr2) {\r\n        hash2[value] = (hash2[value] || 0) + 1\r\n    }  \r\n\r\n    for (var key in hash1) {\r\n        if (!(key ** 2 in hash2)) {\r\n            return false;\r\n        }\r\n\r\n        if (hash2[key ** 2] !== hash1[key]) {\r\n            return false;\r\n        }\r\n    }\r\n\r\n    return true;\r\n}</code></pre>