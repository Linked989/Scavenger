    gg.refineNumber("-1903895931231645695", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
    gg.refineAddress("E40", -1, gg.TYPE_QWORD, gg.SIGN_EQUAL, 0, -1)
    gg.refineNumber("-1903895931231645695", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
