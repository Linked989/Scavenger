function bflash()
  if risa10 == on then
    function split(A0_2708, A1_2709)
      local L2_2710, L3_2711, L4_2712
      L2_2710 = 1
      L3_2711 = 1
      L4_2712 = {}
      while true do
        if not string.find(A0_2708, A1_2709, L2_2710) then
          L4_2712[L3_2711] = string[L0_2693("9d9f8c")](A0_2708, L2_2710, string.len(A0_2708))
          break
        end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        L4_2712[L3_2711] = string.sub(A0_2708, L2_2710, string.find(A0_2708, A1_2709, L2_2710) - 1)
        L2_2710 = string.find(A0_2708, A1_2709, L2_2710) + string.len(A1_2709)
        L3_2711 = L3_2711 + 1
      end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
      return L4_2712
    end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
    
    function xgxc(A0_2713, A1_2714)
      local L2_2715, L3_2716, L4_2717, L5_2718
      L2_2715 = 1
      L3_2716 = #A1_2714
      do
        do
          for i = 1, #A1_2714 do
            xgpy = A0_2713 + A1_2714[i].offset
            xglx = A1_2714[i].type
            xgsz = A1_2714[i].value
            xgdj = A1_2714[i].freeze
            if xgdj == nil or xgdj == "" then
              gg.setValues({
                [1] = {}
              })
            else
              gg.addListItems({
                [1] = {}
              })
            end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
            xgsl = xgsl + 1
            xgjg = true
          end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
      end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
    end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
    
    function xqmnb(A0_2719)
      gg.clearResults()
      gg.setRanges(A0_2719[1].memory)
      gg.searchNumber(A0_2719[3].value, A0_2719[3].type)
      if gg.getResultCount() == 0 then
        gg.toast(A0_2719[2].name .. " ")
      else
        gg.refineNumber(A0_2719[3].value, A0_2719[3].type)
        gg.refineNumber(A0_2719[3].value, A0_2719[3].type)
        gg.refineNumber(A0_2719[3].value, A0_2719[3].type)
        if gg.getResultCount() == 0 then
          gg.toast(A0_2719[2].name .. "开启失败")
        else
          sl = gg.getResults(999999)
          sz = gg.getResultCount()
          xgsl = 0
          if 999999 < sz then
            sz = 999999
          end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
          do
            do
              for i = 1, sz do
                pdsz = true
                do
                  do
                    for i = 4, #A0_2719 do
                      if pdsz == true then
                        pysz = {}
                        pysz[1] = {}
                        szpy = gg.getValues(pysz)
                        pdpd = A0_2719[i].lv .. ";" .. szpy[1].value
                        szpd = split(pdpd, ";")
                        tzszpd = szpd[1]
                        pyszpd = szpd[2]
                        if tzszpd == pyszpd then
                          pdjg = true
                          pdsz = true
                        else
                          pdjg = false
                          pdsz = false
                        end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
                      end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
                    end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
                  end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
                end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
                if pdjg == true then
                  szpy = sl[i].address
                  xgxc(szpy, qmxg)
                end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
              end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
            end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
          end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
          if xgjg == true then
            gg.toast(A0_2719[2].name .. "开启成功，一共修改" .. xgsl .. "条数据")
          else
            gg.toast(A0_2719[2].name .. "未搜索到数据，开启失败")
          end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
      end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
    end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
    
    function SearchWrite(A0_2720, A1_2721, A2_2722)
      gg.clearResults()
      gg.setVisible(false)
      gg.searchNumber(A0_16[1][1], A2_18)
      gg.clearResults()
      if gg.getResultCount() > 0 then
        do
          for i, i in ipairs((gg.getResults((gg.getResultCount())))) do
          end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        do
          do
            for i = 2, #A0_16 do
              do
                for i, i in ipairs((gg.getResults((gg.getResultCount())))) do
                end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
              end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
              do
                for i, i in ipairs((gg.getValues({}))) do
                  if tostring(i.value) ~= tostring(A0_16[i][1]) then
                  end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
                end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
              end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
            end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
          end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        do
          for i, i in ipairs((gg.getResults((gg.getResultCount())))) do
          end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
        if #{} > 0 then
          do
            do
              for i = 1, #{} do
                do
                  for i, i in ipairs(A1_17) do
                    offset = i[2] - A0_16[1][2]
                    if i[3] == true then
                      gg.addListItems({})
                    end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
                  end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
                end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
              end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
            end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
          end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
          gg.setValues({})
          gg.toast("开启成功，一共修改" .. #{} .. "条数据")
          gg.addListItems({})
        else
          gg.toast("未搜索到数据，开启失败", false)
          return false
        end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
      else
        gg.toast("Not Found")
        return false
      end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
    end --꙰ ﴾ 𐌶𐍅𐍄 ꗃ ﴿ --꙰
    
    qmnb = {
      {},
      {
        name = "Please Wait..!!!"
      },
      {value = 5.2806111E-40},
      {lv = 4.7592633E21, offset = -44},
      {lv = 3.0230535E23, offset = 4},
      {lv = 2.2963078E-41, offset = 8}
    }
    qmxg = {
      {value = 99999, offset = 160}
    }
    xqmnb(qmnb)
    qmnb = {
