function bflash()
  if risa10 == on then
    function split(A0_2708, A1_2709)
      local L2_2710, L3_2711, L4_2712
      L2_2710 = 1
      L3_2711 = 1
      L4_2712 = {}
      while true do
        if not string.find(A0_2708, A1_2709, L2_2710) then
          L4_2712[L3_2711] = string[L0_2693("9d9f8c")](A0_2708, L2_2710, string.len(A0_2708))
          break
        end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
        L4_2712[L3_2711] = string.sub(A0_2708, L2_2710, string.find(A0_2708, A1_2709, L2_2710) - 1)
        L2_2710 = string.find(A0_2708, A1_2709, L2_2710) + string.len(A1_2709)
        L3_2711 = L3_2711 + 1
      end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
      return L4_2712
    end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
    
    function xgxc(A0_2713, A1_2714)
      local L2_2715, L3_2716, L4_2717, L5_2718
      L2_2715 = 1
      L3_2716 = #A1_2714
      do
        do
          for i = 1, #A1_2714 do
            xgpy = A0_2713 + A1_2714[i].offset
            xglx = A1_2714[i].type
            xgsz = A1_2714[i].value
            xgdj = A1_2714[i].freeze
            if xgdj == nil or xgdj == "" then
              gg.setValues({
                [1] = {}
              })
            else
              gg.addListItems({
                [1] = {}
              })
            end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
            xgsl = xgsl + 1
            xgjg = true
          end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
        end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
      end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
    end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
    
    function xqmnb(A0_2719)
      gg.clearResults()
      gg.setRanges(A0_2719[1].memory)
      gg.searchNumber(A0_2719[3].value, A0_2719[3].type)
      if gg.getResultCount() == 0 then
        gg.toast(A0_2719[2].name .. " ")
      else
        gg.refineNumber(A0_2719[3].value, A0_2719[3].type)
        gg.refineNumber(A0_2719[3].value, A0_2719[3].type)
        gg.refineNumber(A0_2719[3].value, A0_2719[3].type)
        if gg.getResultCount() == 0 then
          gg.toast(A0_2719[2].name .. "å¼€å¯å¤±è´¥")
        else
          sl = gg.getResults(999999)
          sz = gg.getResultCount()
          xgsl = 0
          if 999999 < sz then
            sz = 999999
          end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
          do
            do
              for i = 1, sz do
                pdsz = true
                do
                  do
                    for i = 4, #A0_2719 do
                      if pdsz == true then
                        pysz = {}
                        pysz[1] = {}
                        szpy = gg.getValues(pysz)
                        pdpd = A0_2719[i].lv .. ";" .. szpy[1].value
                        szpd = split(pdpd, ";")
                        tzszpd = szpd[1]
                        pyszpd = szpd[2]
                        if tzszpd == pyszpd then
                          pdjg = true
                          pdsz = true
                        else
                          pdjg = false
                          pdsz = false
                        end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
                      end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
                    end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
                  end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
                end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
                if pdjg == true then
                  szpy = sl[i].address
                  xgxc(szpy, qmxg)
                end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
              end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
            end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
          end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
          if xgjg == true then
            gg.toast(A0_2719[2].name .. "å¼€å¯æˆåŠŸï¼Œä¸€å…±ä¿®æ”¹" .. xgsl .. "æ¡æ•°æ®")
          else
            gg.toast(A0_2719[2].name .. "æœªæœç´¢åˆ°æ•°æ®ï¼Œå¼€å¯å¤±è´¥")
          end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
        end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
      end --ê™° ï´¾ ğŒ¶ğ…ğ„ ê—ƒ ï´¿ --ê™°
