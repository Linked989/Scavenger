local bruh = game.Players.LocalPlayer:GetMouse()
db = true
bruh.KeyDown:connect(function(k)
	k = k:lower()
	if k == "p" then
		if db == true then
			MainFrame.Visible = true
		end
	end
end)

local cheese = game.Players.LocalPlayer:GetMouse()
db = true
bruh.KeyDown:connect(function(k)
	k = k:lower()
	if k == "m" then
		if db == true then
			MainFrame.Visible = false
		end
	end
end)


